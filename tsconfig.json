{
    "compilerOptions": {
        "declaration": true,
        "declarationMap": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "outDir": "dist",
        "skipLibCheck": true,
        "sourceMap": true,
        "strict": true,
        "target": "es2017"
    },
    "include": ["src"]
}
